{
  "version": 1,
  "routes": [
    { "src": "/(.*).js", "dest": "/$1.js" },
    { "src": "/(.*).css", "dest": "/$1.css" },
    { "src": "/(.*).svg", "dest": "/$1.svg" },
    { "src": "/(.*).png", "dest": "/$1.png" },
    { "src": "/(.*).jpg", "dest": "/$1.jpg" },
    { "src": "/(.*).json", "dest": "/$1.json" },
    { "src": "/*", "dest": "/index.html" }
  ]
}
