# Configuration d'environnement de d√©veloppement pour le service de paiement FloDrama

# Mode d'environnement
NODE_ENV=development

# Configuration AWS
AWS_REGION=eu-west-3
AWS_PROFILE=default

# Tables DynamoDB
SUBSCRIPTION_TABLE=flodrama-payment-service-dev-subscriptions
PAYMENT_HISTORY_TABLE=flodrama-payment-service-dev-payment-history
ANALYTICS_TABLE=flodrama-payment-service-dev-analytics

# Configuration PayPal
PAYPAL_CLIENT_ID=sb
PAYPAL_CLIENT_SECRET=sandbox_secret
PAYPAL_MODE=sandbox

# Configuration CORS
CORS_ALLOWED_ORIGINS=http://localhost:3000,https://dev.flodrama.com

# Configuration de journalisation
LOG_LEVEL=debug
