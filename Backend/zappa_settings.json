{
  "production": {
    "app_function": "api_server.app",
    "aws_region": "eu-west-3",
    "profile_name": "default",
    "project_name": "flodrama-api",
    "runtime": "python3.9",
    "s3_bucket": "flodrama-zappa-deploy",
    "environment_variables": {
      "FLODRAMA_S3_BUCKET": "flodrama-dump",
      "FLODRAMA_S3_KEY": "content.json"
    },
    "timeout_seconds": 30,
    "memory_size": 512,
    "keep_warm": false
  }
}
